{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Session Buddy",
  "version": "1.0",
  "description": "Save, organize, and recall your browser tabs",
  "default_locale": "en",
  "minimum_chrome_version": "109",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "assets/logo-32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "38": "assets/logo-38.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "incognito": "spanning",
  "commands": {
    "save-session": {
      "description": "Save tabs"
    },
    "clear-item-selections": {
      "description": "Clear selection"
    }
  },
  "web_accessible_resources": [{
    "resources": [
      "session-buddy.html",
      "main.html",
      "redirect.js",
      "main.bundle.js",
      "styles.css",
      "set-background-color.js"
    ],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}